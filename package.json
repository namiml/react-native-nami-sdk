{
  "name": "react-native-nami-sdk",
  "version": "3.0.30",
  "description": "React Native Module for Nami - Easy subscriptions & in-app purchases, with powerful built-in paywalls and A/B testing.",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index.tsx",
  "source": "src/index.tsx",
  "types": "lib/typescript/src/index.d.ts",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "react-native-nami-sdk.podspec",
    "!android/build",
    "!ios/build"
  ],
  "sideEffects": false,
  "scripts": {
    "android": "react-native run-android",
    "android-clean": "cd android && rm -rf .gradle && rm -rf .idea && rm -rf android.iml && rm -rf local.properties",
    "ios": "react-native run-ios",
    "ios-setup": "cd ios && pod install",
    "ios-clean": "cd ios && rm -rf Pods && rm -rf build",
    "ios-setup-clean": "cd ios && rm -rf Pods && rm -rf build  && pod update && pod install",
    "typescript": "tsc --noEmit",
    "lint": "yarn lint-no-fix --fix",
    "lint-no-fix": "eslint --ext '.js,.ts,.tsx' .",
    "prepack": "bob build && node ./scripts/generate-mappings.js",
    "bootstrap": "yarn --cwd examples/Basic && yarn",
    "example": "yarn --cwd examples/Basic"
  },
  "keywords": [
    "in-app-purchase",
    "ios",
    "ipados",
    "tvos",
    "paywall",
    "react-native",
    "storekit",
    "subscriptions",
    "iap",
    "play-billing",
    "payments"
  ],
  "author": {
    "username": "hellonami",
    "name": "Nami ML Inc.",
    "email": "hello@namiml.com"
  },
  "contributors": [
    {
      "name": "Francisco Pena",
      "email": "francisco.pena@namiml.com"
    },
    {
      "name": "Kendall Gelner",
      "email": "kendall.gelner@namiml.com"
    }
  ],
  "license": "SEE LICENSE FILE",
  "standard": {
    "ignore": [
      "examples/*"
    ]
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/core": "^7.20.7",
    "@babel/runtime": "^7.20.7",
    "@callstack/eslint-config": "^13.0.2",
    "@types/node": "^13.1.0",
    "@types/react-dom": "^18.0.8",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.1",
    "@types/jest": "^29.5.2",
    "@types/react-native": "^0.72.2",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.2.3",
    "babel-test": "^0.1.1",
    "eslint": "^8.46.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-local-rules": "^1.3.2",
    "jest": "^26.1.0",
    "jest-file-snapshot": "^0.3.2",
    "metro-react-native-babel-preset": "0.72.3",
    "prettier": "^2.8.7",
    "react": "^17.0.2",
    "react-dom": "18.2.0",
    "react-native": "^0.65.2",
    "react-native-builder-bob": "^0.21.3",
    "react-native-codegen": "^0.0.12",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "all-contributors-cli": "^6.24.0",
    "glob": "^7.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namiml/react-native-nami-sdk.git"
  },
  "homepage": "https://www.namiml.com",
  "bugs": {
    "url": "https://github.com/namiml/react-native-nami-sdk/issues"
  },
  "greenkeeper": {
    "ignore": [
      "expo",
      "react",
      "react-dom",
      "react-native",
      "react-test-renderer",
      "@types/react-native",
      "@types/jest",
      "eslint"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
