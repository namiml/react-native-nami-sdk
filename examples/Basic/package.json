{
  "name": "basic",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules",
    "getsdk": "sh -c 'npm install $(node scripts/print-latest-sdk-pack-name.mjs)'",
    "build": "npm install; rm -rf node_modules/react-native-nami-sdk/examples",
    "android": "react-native run-android",
    "android-clean": "cd android && rm -rf .gradle && rm -rf .idea && rm -rf android.iml && rm -rf local.properties",
    "ios": "react-native run-ios",
    "ios-clean": "cd ios && rm -rf Pods Podfile.lock && rm -rf build",
    "ios-build": "cd ios && pod install",
    "start": "npx react-native start --reset-cache",
    "start:legacy": "NODE_OPTIONS=--openssl-legacy-provider react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test:e2e": "detox test --configuration android.emu.debug",
    "i": "echo 'Examples: \"yarn i both\"- run all setups, \"yarn i ios\"- run ios' && ./install.sh",
    "b": "echo 'Examples: \"yarn b ios\", \"yarn b android\"' && ./building.sh",
    "d": "./scripts/detox.sh"
  },
  "dependencies": {
    "@react-native-community/cli-platform-android": "^12.3.6",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-masked-view/masked-view": "0.3.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.10.1",
    "react": "18.2.0",
    "react-native": "0.73.7",
    "react-native-iap": "10.1.3",
    "react-native-logs": "^5.1.0",
    "react-native-nami-sdk": "file:../../react-native-nami-sdk-3.3.0.tgz",
    "react-native-permissions": "^5.4.1",
    "react-native-reanimated": "3.8.1",
    "react-native-safe-area-context": "^4.10.7",
    "react-native-screens": "3.30.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "detox": "20.34.4",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "resolutions": {
    "react-native-gesture-handler": "2.1.0"
  }
}
